{
  "name": "@dimerapp/edge",
  "version": "3.0.0-1",
  "description": "Edge renderer for rendering dimer markdown AST",
  "main": "build/index.js",
  "type": "module",
  "files": [
    "build/src",
    "build/index.d.ts",
    "build/index.js"
  ],
  "exports": {
    ".": "./build/index.js",
    "./utils": "./build/src/utils.js"
  },
  "scripts": {
    "pretest": "npm run lint",
    "test": "node --loader=ts-node/esm bin/test.ts",
    "clean": "del build",
    "compile": "npm run lint && npm run clean && tsc",
    "build": "npm run compile",
    "prepublishOnly": "npm run build",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "release": "np",
    "version": "npm run build",
    "sync-labels": "github-label-sync --labels .github/labels.json dimerapp/edge"
  },
  "keywords": [
    "dimerapp",
    "markdown",
    "edge"
  ],
  "author": "virk,dimerapp",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@dimerapp/markdown": "^7.0.1-0",
    "@dimerapp/shiki": "^4.0.1-1",
    "@japa/assert": "^1.3.4",
    "@japa/run-failed-tests": "^1.0.7",
    "@japa/runner": "^2.0.9",
    "@japa/spec-reporter": "^1.1.12",
    "@swc/core": "^1.2.220",
    "@types/node": "^18.6.2",
    "del-cli": "^5.0.0",
    "edge.js": "^5.3.7",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-adonis": "^3.0.2",
    "eslint-plugin-prettier": "^4.2.1",
    "github-label-sync": "^2.2.0",
    "husky": "^8.0.1",
    "np": "^7.6.2",
    "prettier": "^2.7.1",
    "ts-dedent": "^2.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "edge-error": "^2.0.8",
    "escape-goat": "^4.0.0",
    "hast-util-to-string": "^2.0.0",
    "html-tags": "^3.2.0",
    "property-information": "^6.1.1",
    "stringify-attributes": "^3.0.0"
  },
  "peerDependencies": {
    "@dimerapp/markdown": "^7.0.1-0"
  },
  "directories": {
    "example": "example",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dimerapp/edge.git"
  },
  "bugs": {
    "url": "https://github.com/dimerapp/edge/issues"
  },
  "homepage": "https://github.com/dimerapp/edge#readme",
  "publishConfig": {
    "access": "public",
    "tag": "prerelease"
  },
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptPackage",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "endOfLine": "auto"
        }
      ]
    }
  },
  "eslintIgnore": [
    "build"
  ],
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "np": {
    "contents": "./",
    "message": "chore(release): %s",
    "branch": "main",
    "anyBranch": false
  }
}
